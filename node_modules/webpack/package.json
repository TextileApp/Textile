    "build:examples": "cd examples && node buildAll.js",
    "cover": "istanbul cover -x '**/*.runtime.js' node_modules/mocha/bin/_mocha",
    "lint": "eslint lib bin hot",
    "lint-files": "npm run lint && npm run beautify-lint",
